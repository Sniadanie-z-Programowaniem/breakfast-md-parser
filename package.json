{
    "name": "morning-md-parser",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev src/index.ts",
        "start": "TS_NODE_FILES=true ts-node src/index.ts",
        "build": "tsc",
        "start:dist": "npm run build && node dist/index.js",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "test:debug": "jest --detectOpenHandles",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "keywords": [],
    "author": "michalczukm@gmail.com",
    "license": "ISC",
    "engines": {
        "node": ">=14.15.4"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "date-fns": "^2.16.1",
        "lodash": "^4.17.20",
        "marked": "^1.2.7"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.1",
        "@types/jest": "^26.0.20",
        "@types/lodash": "^4.14.167",
        "@types/marked": "^1.2.1",
        "@types/node": "^14.14.20",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "husky": "^4.3.6",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1",
        "typescript": "^4.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    }
}
